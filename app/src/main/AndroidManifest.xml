<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="app.com.example.cruck.h2g">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:name=".ToDoApplication"
        android:allowBackup="true"
        android:icon="@mipmap/log"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivityCustomer" />
        <activity android:name=".SignupActivityCustomer" />
        <activity android:name=".CustomerData" />
        <activity
            android:name=".CustomerLandingActivity" />
        <activity android:name=".LoginActivityService" />
        <activity android:name=".SignupActivityService" />
        <activity android:name=".ServiceData" />
        <activity android:name=".ServiceLandingActivity" />
        <activity android:name=".Rating"></activity>
    </application>

</manifest>